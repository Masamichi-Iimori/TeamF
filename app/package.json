{
  "name": "scenepicks",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "env-cmd -f .env._local next dev",
    "dev:mock": "env-cmd -f .env._mock next dev",
    "dev:prod": "env-cmd -f .env._prod next dev",
    "build": "env-cmd -f .env._local next build",
    "build:prod": "env-cmd -f .env._prod next build",
    "start": "env-cmd -f .env._local next start",
    "start:prod": "env-cmd -f .env._prod next start",
    "deploy:prod": "yarn run build:prod && yarn run start:prod",
    "gendoc": "jsdoc lib -p -c jsdoc.conf -d doc -t ./node_modules/docdash"
  },
  "dependencies": {
    "@pixi/filter-drop-shadow": "^3.1.1",
    "axios": "^0.20.0",
    "canvas": "^2.6.1",
    "easing-utils": "^0.0.7",
    "env-cmd": "^10.1.0",
    "firebase": "^7.21.0",
    "firebaseui": "^4.6.1",
    "firebaseui-ja": "^1.0.0",
    "matter-js": "^0.14.2",
    "next": "9.5.3",
    "pixi.js": "^5.3.3",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-modal": "^3.11.2",
    "react-share": "^4.3.0",
    "react-spring": "^8.0.27",
    "react-toast-notifications": "^2.4.0",
    "sass": "^1.26.10",
    "urlsafe-base64": "^1.0.0",
    "rollbar": "^2.19.3"
  },
  "devDependencies": {
    "docdash": "^1.2.0",
    "eslint": "^7.9.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.20.6",
    "eslint-plugin-react-hooks": "^4.1.2",
    "jsdoc": "^3.6.6",
    "jsdoc-external-docs": "https://github.com/cnordhougen/jsdoc-external-docs",
    "miragejs": "^0.1.40",
    "prettier": "^2.1.2"
  }
}
